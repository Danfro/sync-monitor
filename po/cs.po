#: authenticator/main.qml:66 authenticator/main.qml:84
msgid "Accounts"
msgstr "Účty"

#: src/sync-account.cpp:740
msgid "Command not allowed"
msgstr "Nepovolený příkaz"

#: src/sync-account.cpp:762
msgid "Connection certificate has expired"
msgstr "Platnost certifikátu připojení skončila"

#: src/sync-account.cpp:764
msgid "Connection certificate is invalid"
msgstr "Neplatný certifikát připojení"

#: src/sync-account.cpp:760
msgid "Connection timeout"
msgstr "Překročen časový limit pro navázání/udržení spojení"

#: src/sync-account.cpp:745
msgid "Disk full"
msgstr "Zaplněné datové úložiště"

#: src/sync-account.cpp:727
msgid "Fail to configure account"
msgstr "Účet se nezdařilo nastavit"

#: src/sync-account.cpp:768
msgid "Fail to connect with the server"
msgstr "Připojení k serveru se nezdařilo"

#: authenticator/main.qml:94
msgid "Fail to load account information."
msgstr "Informace o účtu se nezdařilo načíst."

#: src/sync-account.cpp:749
msgid "Fail to run \"two-way\" sync"
msgstr "Spuštění „oboustranné“ synchronizace se nezdařilo"

#: authenticator/main.qml:119
msgid "Fail to sync"
msgstr "Synchronizace se nezdařila"

#: src/sync-daemon.cpp:736
#, qt-format
msgid ""
"Fail to sync calendar %1 from account %2.\n"
"%3"
msgstr ""
"Synchronizace kalendáře %1 z účtu %2 se nezdařila.\n"
"%3"

#: src/sync-account.cpp:747
msgid "Fail to sync due some remote problem"
msgstr "Synchronizace se nezdařila kvůli problému na protějšku"

#: src/sync-account.cpp:751
msgid "Fail to sync some items"
msgstr "Některé položky se nepodařilo synchronizovat"

#: src/sync-account.cpp:736
msgid "Forbidden / access denied"
msgstr "Zakázané / přístup odmítnut"

#: src/notify-message.cpp:88
msgid "No"
msgstr "Ne"

#: src/sync-account.cpp:738
msgid "Object not found / unassigned field"
msgstr "Objekt nenalezen / nepřiřazená kolonka"

#: src/sync-account.cpp:753
msgid "Process unexpected die."
msgstr "Proces byl neočekávaně ukončen."

#: src/sync-account.cpp:743
msgid "Proxy authentication required"
msgstr "Je vyžadováno ověření proxy serveru"

#: authenticator/main.qml:88 authenticator/main.qml:123
msgid "Quit"
msgstr "Ukončit"

#: src/sync-account.cpp:771
msgid "Server not found"
msgstr "Server nenalezen"

#: src/sync-account.cpp:756
msgid "Server sent bad content"
msgstr "Server zaslal chybný obsah"

#: src/sync-daemon.cpp:642
#, qt-format
msgid "Start sync: %1 (Calendar)"
msgstr "Zahájení synchronizace: %1 (Kalendáře)"

#: src/sync-account.cpp:758
msgid "Sync canceled"
msgstr "Synchronizace zrušena"

#: src/sync-daemon.cpp:754
#, qt-format
msgid "Sync done: %1 (Calendar)"
msgstr "Synchronizace dokončena: %1 (Kalendáře)"

#: src/sync-daemon.cpp:619 src/sync-daemon.cpp:641 src/sync-daemon.cpp:735
#: src/sync-daemon.cpp:753
msgid "Synchronization"
msgstr "Synchronizace"

#: src/sync-account.cpp:773
msgid "Unknown status"
msgstr "Neznámý stav"

#: src/notify-message.cpp:83
msgid "Yes"
msgstr "Ano"

#: src/sync-daemon.cpp:620
msgid ""
"Your access key is not valid anymore. Do you want to re-authenticate it?."
msgstr "Váš přístupový klíč už není platný. Přejete si znovu se ověřit?"

#: authenticator/main.qml:160
msgid ""
"Your account failed to authenticate while syncing. Please click below to re-"
"authenticate."
msgstr ""
"Ověření se k vašemu účtu se v průběhu synchronizace nezdařilo. Kliknutím na "
"níže uvedený odkaz provedete opětovné ověření se."
